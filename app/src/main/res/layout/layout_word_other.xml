<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/common_google_signin_btn_icon_light_normal_background">

    <TextView
            android:id="@+id/txt_other_sname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이름  : "
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            app:layout_constraintBottom_toTopOf="@id/txt_other_smaen"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/txt_other_name"
            app:layout_constraintTop_toTopOf="parent" />

    <TextView
            android:id="@+id/txt_other_smaen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:text="뜻 : "
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/txt_other_mean"
            app:layout_constraintTop_toBottomOf="@id/txt_other_sname" />

    <TextView
            android:id="@+id/txt_other_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            app:layout_constraintBaseline_toBaselineOf="@id/txt_other_sname"
            app:layout_constraintBottom_toTopOf="@id/txt_other_mean"
            app:layout_constraintLeft_toRightOf="@id/txt_other_sname"
            app:layout_constraintTop_toTopOf="parent" />

    <TextView
            android:id="@+id/txt_other_mean"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            app:layout_constraintBaseline_toBaselineOf="@id/txt_other_smaen"
            app:layout_constraintLeft_toRightOf="@id/txt_other_smaen"
            app:layout_constraintTop_toBottomOf="@id/txt_other_name" />
</androidx.constraintlayout.widget.ConstraintLayout>